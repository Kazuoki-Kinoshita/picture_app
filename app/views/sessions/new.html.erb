<% flash.each do |key, value|%>
  <%= content_tag(:div, value, class: "#{key}") %>
<% end %>

<%= form_with(scope: :session, url: sessions_path, local:true) do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email %></br>
  <%= form.label :password %>
  <%= form.password_field :password %></br>
  <%= form.submit "ログイン" %>
<% end %>